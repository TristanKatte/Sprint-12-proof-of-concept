<%- include('./partials/head') %>
<head>
    <title>Google Analytics | Fresk Digital</title>
</head>

<%- include('./partials/nav') %>


<main>
<section id="google-section">
  
 
    
    <article>
      <h3>Google Analytics</h3>
        <p>showcase of clicks by active users</p>
        
        <% googleAd.rows.forEach(function(row) { %>
          <p><%= row.dimensionValues[0].value %>: <%= row.metricValues[0].value %></p>

   

      <% }) %>

    </article>

  <div>
    <canvas id="myChart" width="750" height="750"></canvas>
  </div>
  
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  
  <script>

const colourTheme = [
    '#B9FFB7',
    '#FF7535',
    '#5E43FF'
  ];
  
  const borderColour = ['#302A2F;'];
  const borderWidth = 1.5;
    const ctx = document.getElementById('myChart');
  
    new Chart(ctx, {
      type: 'doughnut',
      data: {
        labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'], 
        datasets: [{
          label: '# of Votes',
          data: [12, 19, 3, 5, 2, 3],
          borderWidth: 1
        }]
      },
      options: {
        scales: {
          y: {
            beginAtZero: true
          }
        }
      }
    });
  </script>

</section>
</main>


