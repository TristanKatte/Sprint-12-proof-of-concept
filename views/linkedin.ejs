<%- include('./partials/head') %>

<head>
    <title>Linkedin Analytics | Fresk Digital</title>
</head>

<div class="transition transition-1 is-active">
  <img src="/img/fresk-digital-snow.png" alt="Fresk Digital Logo">
  <span class="loader"></span>
</div>

<%- include('./partials/nav') %>

<section id="linkedin-section">

    <article>
        <h3>Linkedin analytics</h3>
          <p>showcase of clicks by active users</p>
    </article>

      <div>
        <canvas id="myChart1" width="750" height="750"></canvas>
      </div>

      <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  
      <script>
        const ctx1 = document.getElementById('myChart1');
      
        new Chart(ctx1, {
          type: 'bar',
          data: {
            labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'], 
            datasets: [{
              label: '# of Votes',
              data: [12, 19, 3, 5, 2, 3],
              borderWidth: 1
            }]
          },
          options: {
            scales: {
              y: {
                beginAtZero: true
              }
            }
          }
        });
      </script>

</section>


<%- include('./partials/footer') %>