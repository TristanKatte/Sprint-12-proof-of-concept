<%- include('./partials/head') %>

<div class="transition transition-1 is-active">
    <img src="/img/fresk-digital-snow.png" alt="Fresk Digital Logo">
    <span class="loader"></span>
</div>

<main>
 
  <section class="login-section">
    <form class="login" action="/dashboard" method="get">
      <h2>Log hier in</h2>
    
      <label for="username">Naam:</label>
      <input type="email" id="username" name="username">
     
      <label for="password">Wachtwoord:</label>
      <input type="password" id="password" name="password" required>
      
      <button class="login-btn" type="submit" value="Log in">Log in</button>
    </form>
  </section>

</main>

<%- include('./partials/footer') %>